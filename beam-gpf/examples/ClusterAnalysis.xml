<graph>
    <id>ClusterAnalysis</id>

    <node>
        <id>read</id>
        <operator>ReadProduct</operator>
        <parameters>
            <filePath>${inputFile}</filePath>
        </parameters>
    </node>

    <node>
        <id>cluster</id>
        <operator>ClusterAnalysis</operator>
        <sources>
            <sourceProduct>read</sourceProduct>
        </sources>
        <parameters>
            <features>
                <feature>reflec_2</feature>
                <feature>reflec_4</feature>
                <feature>reflec_6</feature>
                <feature>reflec_12</feature>
            </features>
        </parameters>
    </node>

    <node>
        <id>write</id>
        <operator>WriteProduct</operator>
        <sources>
            <input>cluster</input>
        </sources>
        <parameters>
            <filePath>${outputFile}</filePath>
            <formatName>BEAM-DIMAP</formatName>
        </parameters>
    </node>

</graph>