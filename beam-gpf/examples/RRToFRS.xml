<graph>
    <id>RR_to_FRS</id>

    <node>
        <id>frs</id>
        <operator>org.esa.beam.framework.gpf.operators.common.ReadProductOp$Spi</operator>
        <parameters>
            <filePath>${inputFile1}</filePath>
        </parameters>
    </node>

    <node>
        <id>rr</id>
        <operator>org.esa.beam.framework.gpf.operators.common.ReadProductOp$Spi</operator>
        <parameters>
            <filePath>${inputFile2}</filePath>
        </parameters>
    </node>

    <node>
        <id>rrtofrs</id>
        <operator>org.esa.beam.framework.gpf.operators.meris.RRToFRSOp$Spi</operator>
        <sources>
            <frs>frs</frs>
            <rr>rr</rr>
        </sources>
    </node>

    <node>
        <id>write</id>
        <operator>org.esa.beam.framework.gpf.operators.common.WriteProductOp$Spi</operator>
        <sources>
            <input>rrtofrs</input>
        </sources>
        <parameters>
            <filePath>${outputFile1}</filePath>
        </parameters>
    </node>
</graph>