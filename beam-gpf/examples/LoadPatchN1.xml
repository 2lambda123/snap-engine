<graph>
    <id>LoadWrite</id>
    <node>
        <id>load</id>
        <operator>org.esa.beam.framework.gpf.operators.common.ReadProductOp$Spi</operator>
        <parameters>
            <filePath>${inputFile1}</filePath>
        </parameters>
    </node>
    <node>
        <id>write</id>
        <operator>org.esa.beam.framework.gpf.operators.meris.N1PatcherOp$Spi</operator>
