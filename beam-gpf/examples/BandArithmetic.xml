<graph>
    <id>landcons</id>

    <node>
        <id>l1</id>
        <operator>ReadProduct</operator>
        <parameters>
            <filePath>${inputFile1}</filePath>
        </parameters>
    </node>

    <node>
        <id>ba</id>
        <operator>BandArithmetic</operator>
        <sources>
            <l1>l1</l1>
        </sources>
        <parameters>
            <bandDescriptor>
                <name>landcons</name>
                <expression>l1_flags.LAND_OCEAN</expression>
                <type>byte</type>
            </bandDescriptor>
        </parameters>
    </node>

    <node>
        <id>write</id>
        <operator>WriteProduct</operator>
        <sources>
            <input>ba</input>
        </sources>
        <parameters>
            <filePath>${outputFile1}</filePath>
        </parameters>
    </node>
</graph>